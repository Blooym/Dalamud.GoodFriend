{
    "dockerFile": "Dockerfile",
    "updateRemoteUserUID": true,
    "containerUser": "dev",
    "remoteUser": "dev",
    "postAttachCommand": "cd ./src/Server && dotnet build && cd ../Plugin && dotnet build",
    "containerEnv": {
        "DOTNET_TELEMTRY_OPTOUT": "1",
        "DOTNET_SKIP_FIRST_TIME_EXPERIENCE": "1"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-dotnettools.csharp"
            ]
        }
    }
}